{
  "name": "jtdsoft/essentials",
  "description": "Essentials By Jordan Dobrev",
  "keywords": [
    "essentials",
    "laravel",
    "php",
    "sdk"
  ],
  "license": "JordanDobrev",
  "type": "package",
  "require": {
    "guzzlehttp/guzzle": "~6.0",
    "doctrine/inflector": "~1.0",
    "illuminate/database": "^5.4",
    "illuminate/validation": "^5.4",
    "illuminate/translation": "^5.4",
    "illuminate/support": "^5.4",
    "umpirsky/country-list": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "phpspec/phpspec": "~2.1",
    "mockery/mockery": "*",
    "fzaninotto/faker": "1.4.0",
    "squizlabs/php_codesniffer": "3.*",
    "theseer/phpdox": "*"
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "autoload": {
    "psr-4": {
      "JTDSoft\\Essentials\\": "src/"
    },
    "files": [
      "src/helpers.php",
      "src/Laravel/helpers.php"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
